<p id="notice"><%= notice %></p>

<h1>Abilities</h1>
<div>
</div>

<div class='row'>
  <div class="col s12">
    <ul class="tabs">
      <li class="tab col s3"><a class="active" href="#implemented">Implemented</a></li>
      <li class="tab col s3"><a href="#not_implemented">Not Implemented</a></li>
    </ul>
  </div>
  <div id="implemented" class="col s12"><%= render 'table', abilities: @abilities.select {|a| a[:is_implemented] } %></div>
  <div id="not_implemented" class="col s12"><%= render 'table', abilities: @abilities.reject {|a| a[:is_implemented]  }%></div>
</div>

<script>
  $('.tabs').tabs();
</script>
