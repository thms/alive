<h1>Team Matches</h1>
<%= render 'team_matches/data', team: @t1 %>
<%= render 'team_matches/data', team: @t2 %>
<b>Stats</b>
<div><%= @stats %></div>
<b>Survivors</b>
<div><%= @survivors1 %></div>
<div><%= @survivors2 %></div>
<b>Health</b>
<div><%= @t1.health %></div>
<div><%= @t2.health %></div>
<div><%= TQTeamStrategy.stats %>
<div><%= @graph.html_safe %></div>
<% if @logs.size < 10 %>
<div><%= EventSink.events %></div>
<div><%= render partial: 'logs', logs: @events %></div>
<% end %>
